
function iter(a, i) {
	if (i < a.length()) {
		var v = a[i];
		i = i + 1;
		return :[i, v];
	}
	return nil;
}

function ipairs (a) {
	return :[iter, a, 0];
}

function iterator(a) {
	var i = 0;
	return function() {
		if (i < a:length()) {
			i = i + 1;
			return a[i- 1];
		}
		return nil;
	};
}

